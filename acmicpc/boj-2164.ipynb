   "source": [
    "from collections import deque\n",
    "\n",
    "N = int(input())\n",
    "\n",
    "que = deque()\n",
    "for i in range(1, N+1):\n",
    "    que.appendleft(i)\n",
    "    \n",
    "while len(que) != 1:\n",
    "    que.pop()\n",
    "    tmp = que.pop()\n",
    "    que.appendleft(tmp)\n",
    "    \n",
    "print(que.pop())"
   ]
  }
 ],
